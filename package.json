{
  "dependencies": {
    "express": "^4.16.3"
  },
  "scripts": {
    "start": "node server",
    "dev": "concurrently --kill-others-on-fail 'yarn iridescent' 'yarn server'",
    "iridescent": "iridescent --debug --owner maddox --repo lol --branch iri --token $GITHUB_TOKEN --pusher 82262d9955f278d93d51 --host https://public-noodle.now.sh",
    "server": "nodemon server"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4",
    "noodle-iridescent-cli": "^1.0.2",
    "noodle-it": "^0.0.2"
  },
  "now": {
    "type": "docker",
    "public": true,
    "env": {
      "GITHUB_TOKEN": "@github_token"
    },
    "features": {
      "cloud": "v1"
    }
  }
}
